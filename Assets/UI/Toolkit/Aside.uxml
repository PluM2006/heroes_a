                            <ui:VisualElement name="image" class="aside_menu_buttonimg" style="background-image: url(&apos;project://database/Assets/UI/Sprites/Buttons/Setting%201.png?fileID=2800000&amp;guid=27ae874c69d2c1a40a3e8f8200a846dd&amp;type=3#Setting 1&apos;);" />
