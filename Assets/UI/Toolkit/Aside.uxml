                            <ui:VisualElement name="image" class="aside_menu_buttonimg" style="background-image: url(&apos;project://database/Assets/UI/Sprites/Buttons/GoHero.png?fileID=2800000&amp;guid=2732a5281556f7a40b6d9efbdae34d88&amp;type=3#GoHero&apos;);" />
