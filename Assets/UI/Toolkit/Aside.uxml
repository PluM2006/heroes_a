                <ui:VisualElement name="img" class="footer_box_img" style="background-image: url(&apos;project://database/Assets/UI/Sprites/Buttons/GemFooter.png?fileID=2800000&amp;guid=8839a2b8b6d3dd846ba072fd1278082c&amp;type=3#GemFooter&apos;);" />
                <ui:Label tabindex="-1" text="1230" display-tooltip-when-elided="true" name="GemValue" class="footer_box_text" />
