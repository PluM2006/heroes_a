            <ui:VisualElement name="ActionList" class="aside_blok_list" style="align-items: stretch; flex-grow: 1;">
                <ui:VisualElement style="flex-direction: row; flex-grow: 0; flex-wrap: wrap; flex-shrink: 0; width: 100%;">
                    <ui:VisualElement style="width: 50%;">
                        <ui:Button display-tooltip-when-elided="true" name="ButtonGoHero" class="button_bg button_action aside_button">
