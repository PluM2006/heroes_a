                <ui:Label tabindex="-1" text="1230" display-tooltip-when-elided="true" name="MercuryValue" class="footer_box_text" />
            </ui:VisualElement>
            <ui:VisualElement name="Diamond" class="footer_box">
                <ui:VisualElement name="img" class="footer_box_img" style="background-image: url(&apos;project://database/Assets/UI/Sprites/Buttons/DiamondFooter.png?fileID=2800000&amp;guid=a0174887094951b439b8fda75bf0b23b&amp;type=3#DiamondFooter&apos;);" />
                <ui:Label tabindex="-1" text="1230" display-tooltip-when-elided="true" name="DiamondValue" class="footer_box_text" />
            </ui:VisualElement>
            <ui:VisualElement name="Sulfur" class="footer_box">
                <ui:VisualElement name="img" class="footer_box_img" style="background-image: url(&apos;project://database/Assets/UI/Sprites/Buttons/SulfurFooter.png?fileID=2800000&amp;guid=a074348c7044f044d807ba85164a94f4&amp;type=3#SulfurFooter&apos;);" />
                <ui:Label tabindex="-1" text="1230" display-tooltip-when-elided="true" name="SulfurValue" class="footer_box_text" />
